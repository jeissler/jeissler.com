@import "tailwindcss";

@theme {
    --color-brand: #f4efe8
}

:root {
  --sans-serif-font:
    system-ui, -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto,
    "Helvetica Neue", Arial, sans-serif;
}

html {
  font-family: "DM Sans", var(--sans-serif-font);
}
h1,
h2,
h3 {
  font-family: "Space Grotesk", var(--sans-serif-font);
}
