{
  "name": "jeissler",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "browser-sync": "^2.18.8",
    "flexboxgrid": "^6.3.1",
    "nodemon": "^1.11.0",
    "normalize.css": "^5.0.0",
    "npm-run-all": "^4.0.2",
    "postcss": "^5.2.14",
    "postcss-cli": "^3.0.0-beta",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-pxtorem": "^4.0.0",
    "sanitize.css": "^4.1.0",
    "scrollmagic": "^2.0.5",
    "webpack": "^2.2.1"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "scripts": {
    "js:compile": "webpack --watch",
    "css:compile": "postcss --use postcss-import --use postcss-cssnext --use postcss-pxtorem --config postcss.config.json --map --output assets/bundle.css src/styles/entry.pcss",
    "css:watch": "nodemon --watch src --ext pcss --exec 'npm run css:compile'",
    "serve": "browser-sync start -s -f './index.html' './assets'",
    "start": "npm-run-all --parallel js:compile css:compile css:watch serve"
  },
  "dependencies": {
    "animate.css": "^3.5.2"
  }
}
